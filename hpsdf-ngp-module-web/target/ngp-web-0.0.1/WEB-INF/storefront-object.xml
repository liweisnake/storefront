                    <action-name>viewrecursive</action-name>
                    <unchecked/>
                </policy-permission>
                <policy-permission>
                    <role-name>User</role-name>
                    <action-name>viewrecursive</action-name>
                </policy-permission>
            </security-constraint>
            <window>
                <window-name>News</window-name>
                <instance-ref>NewsPortletInstance2</instance-ref>
                <region>left</region>
