			<compnentType>TextField</compnentType>
			<required>false</required>
		</dataField>

		<dataField>
			<name>country</name>
			<modelType>domain</modelType>
			<dataType>String</dataType>
			<compnentType>DropDownChoice</compnentType>
			<sourcId>countryList</sourcId>
			<required>false</required>
		</dataField>

		<dataField>
			<name>language</name>
			<modelType>domain</modelType>
			<dataType>String</dataType>
			<compnentType>DropDownChoice</compnentType>
			<sourcId>userDefinedSourceOne</sourcId>
			<required>false</required>
		</dataField>

		<dataField>
			<name>gender</name>
			<modelType>domain</modelType>
			<dataType>String</dataType>
			<compnentType>RadioChoice</compnentType>
			<sourcId>genderList</sourcId>
			<required>false</required>
		</dataField>

		<dataField>
			<name>comment</name>
			<modelType>eav</modelType>
			<dataType>String</dataType>
