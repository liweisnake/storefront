			<modelType>domain</modelType>
			<dataType>String</dataType>
			<compnentType>RadioChoice</compnentType>
			<sourcId>userDefinedSourceOne</sourcId>
			<required>false</required>
		</dataField>
	</dataFields>
	<dataSources>
		<appServiceSource>
			<sourcId>countryList</sourcId>
			<className>infoServiceImpl</className>
			<methodName>getCountrys</methodName>
			<keyName>id</keyName>
			<valueName>name</valueName>
		</appServiceSource>

		<appServiceSource>
			<sourcId>languageList</sourcId>
			<className>infoServiceImpl</className>
			<methodName>getLanguages</methodName>
			<keyName>id</keyName>
			<valueName>name</valueName>
		</appServiceSource>

		<constDefinedSource>
			<sourcId>genderList</sourcId>
			<holderName>com.hp.sdf.ngp.ui.page.myportal.Gender
			</holderName>
			<idMethodName>getGenderIdList</idMethodName>
			<valueMethodName>getGenderMap</valueMethodName>
		</constDefinedSource>

		<constDefinedSource>
			<sourcId>genderListTwo</sourcId>
			<holderName>com.hp.sdf.ngp.ui.page.myportal.Gender
			</holderName>
			<idMethodName>getGenderIdList</idMethodName>
			<valueMethodName>getGenderMap</valueMethodName>
		</constDefinedSource>

		<userDefinedSource>
			<keyName>id</keyName>
			<valueName>value</valueName>
			<sourcId>userDefinedSourceOne</sourcId>
			<pairs>
				<pair>
					<id>1</id>
					<value>language</value>
				</pair>
				<pair>
					<id>2</id>
					<value>language2</value>
				</pair>
				<pair>
					<id>3</id>
					<value>language3</value>
				</pair>
			</pairs>
		</userDefinedSource>

