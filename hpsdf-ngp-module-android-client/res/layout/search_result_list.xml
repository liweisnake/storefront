<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout android:id="@+id/searchResultAssetBrief"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<ImageView android:id="@+id/searchResultAssetBriefImg"
		android:adjustViewBounds="true" android:layout_width="56px"
		android:layout_height="60px" android:layout_x="10px" android:layout_y="18px" />
	<TextView android:id="@+id/searchResultAssetBriefName"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="TextView" android:layout_x="84px" android:layout_y="19px">
	</TextView>
	<TextView android:id="@+id/searchResultAssetBriefComName"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="TextView" android:layout_x="84px" android:layout_y="53px">
	</TextView>
	<RatingBar android:id="@+id/searchResultAssetBriefRatingbar" style="?android:attr/ratingBarStyleSmall"
		android:layout_marginLeft="5dip" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_gravity="center_vertical"
		android:layout_x="245px" android:layout_y="19px" />
	<TextView android:id="@+id/searchResultAssetBriefPrice"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="TextView" android:layout_x="245px" android:layout_y="54px">
	</TextView>
</AbsoluteLayout>
