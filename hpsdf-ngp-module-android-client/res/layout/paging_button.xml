<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout android:id="@+id/assetBrief"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<Button android:id="@+id/previousBtn" style="?android:attr/buttonStyleSmall"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="Previous" android:layout_x="10px" android:layout_y="10px">
	</Button>
	<Button android:id="@+id/nextBtn" style="?android:attr/buttonStyleSmall"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="Next" android:layout_x="80px" android:layout_y="10px">
	</Button>

	<TextView android:id="@+id/pagingInfo" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_x="140px"
		android:layout_y="10px">
	</TextView>
</AbsoluteLayout>
